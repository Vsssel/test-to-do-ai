{
  "name": "to-do-app-ai",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "db:migrate": "tsx packages/db/migrate.ts"
  },
  "devDependencies": {
    "@types/pg": "^8.15.6",
    "drizzle-kit": "^0.31.7",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bullmq": "^5.65.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "nodemailer": "^7.0.11",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "zod": "^4.1.12"
  }
}
